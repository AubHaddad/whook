{
  "name": "whook",
  "version": "0.1.0",
  "description": "Build strong and efficient REST web services.",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register src/**/*.mocha.js src/*.mocha.js tests/*.mocha.js",
    "compile": "babel src --out-dir=dist",
    "prepublish": "npm run compile",
    "coveralls": "node --harmony ./node_modules/istanbul/lib/cli.js cover _mocha --report lcovonly -- --compilers js:babel/register src/*.mocha.js tests/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cover": "istanbul cover --report html _mocha -- --compilers js:babel/register src/*.mocha.js tests/*.mocha.js -R spec -t 5000",
    "lint": "eslint src/*.js src/**/*.js tests/*.js",
    "cli": "env NPM_RUN_CLI=1"
  },
  "keywords": [
    "REST",
    "HTTP",
    "server",
    "web",
    "services"
  ],
  "author": "Nicolas Froidure",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nfroidure/whook.git"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/whook/issues"
  },
  "homepage": "https://github.com/nfroidure/whook",
  "devDependencies": {
    "babel": "^5.6.7",
    "coveralls": "^2.11.2",
    "eslint": "^1.3.1",
    "eslint-config-simplifield": "^1.1.0",
    "istanbul": "^0.3.16",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "^0.0.2",
    "neatequal": "^1.0.0",
    "sf-time-mock": "^1.0.2",
    "sinon": "^1.15.3",
    "streamtest": "^1.2.1",
    "supertest": "^1.0.1"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "miniquery": "^1.1.0",
    "yerror": "^1.0.1"
  },
  "peerDependencies": {
    "esprima": "^2.4.1"
  }
}
