{
  "name": "whook",
  "version": "2.0.0",
  "description": "Build strong and efficient REST web services.",
  "metapak": {
    "configs": [
      "main",
      "readme",
      "codeclimate",
      "travis",
      "eslint",
      "babel",
      "jest",
      "jsarch",
      "jsdocs"
    ],
    "data": {
      "files": "'src/**/*.js'",
      "ignore": [
        "dist"
      ]
    }
  },
  "main": "dist/index.js",
  "scripts": {
    "architecture": "jsarch 'src/**/*.js' > ARCHITECTURE.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "compile": "babel src --out-dir=dist",
    "cover": "npm run jest -- --coverage",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "doc": "echo \"# API\" > API.md; jsdoc2md 'src/**/*.js' >> API.md",
    "jest": "NODE_ENV=test jest",
    "lint": "eslint 'src/**/*.js'",
    "metapak": "metapak",
    "precz": "npm t && npm run lint && npm run metapak -- -s",
    "prettier": "prettier --write 'src/**/*.js'",
    "preversion": "npm t && npm run lint && npm run metapak -- -s && npm run compile",
    "start": "node bin/server.js",
    "test": "echo \"WARNING: No tests specified\" && npm run jest",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "keywords": [
    "REST",
    "HTTP",
    "server",
    "web",
    "services"
  ],
  "author": {
    "name": "Nicolas Froidure",
    "email": "nicolas.froidure@insertafter.com",
    "url": "http://insertafter.com/en/index.html"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nfroidure/whook.git"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/whook/issues"
  },
  "homepage": "https://github.com/nfroidure/whook",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "caporal": "^0.10.0",
    "commitizen": "^2.10.1",
    "conventional-changelog-cli": "^2.0.5",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "debug": "^4.0.1",
    "eslint": "^5.6.0",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.6.0",
    "jsarch": "^1.3.0",
    "jsdoc-to-markdown": "^4.0.1",
    "metapak": "1.0.3",
    "metapak-nfroidure": "7.1.1",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "common-services": "^3.2.0",
    "dotenv": "^6.1.0",
    "internal-ip": "^3.0.1",
    "knifecycle": "^3.1.0",
    "portfinder": "^1.0.17",
    "swagger-http-router": "^2.0.0"
  },
  "contributors": [],
  "engines": {
    "node": ">=6.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli",
      "eslint",
      "eslint-config-prettier",
      "prettier",
      "jest",
      "coveralls",
      "jsarch",
      "jsdoc-to-markdown"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "6.9.5"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
